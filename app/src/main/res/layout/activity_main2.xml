<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.upperskills.test.Main2Activity">

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/shell"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <EditText
        android:id="@+id/marque"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Marque"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:padding="7dp"
        android:textColor="#000000"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
        android:background="@drawable/oval"
        />
    <EditText
        android:id="@+id/modele"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="ModÃ©le"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="205dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:padding="7dp"
        android:textColor="#000000"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
        android:background="@drawable/oval"
        />
    <TextView
        android:id="@+id/dateVoiture"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Date"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="260dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:padding="7dp"
        android:textColor="#000000"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
        android:background="@drawable/oval"
        />

    <EditText
        android:id="@+id/email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Email"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="375dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:padding="7dp"
        android:textColor="#000000"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
        android:background="@drawable/oval"
        />
    <EditText
        android:id="@+id/cin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="CIN"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="430dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:padding="7dp"
        android:textColor="#000000"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
        android:background="@drawable/oval"
        />
    <EditText
        android:id="@+id/adresse"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Adresse"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="490dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:padding="7dp"
        android:textColor="#000000"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
        android:background="@drawable/oval"
        />

    <RadioButton

        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/diesel"
        android:id="@+id/diesel"
        android:layout_gravity="right|center_vertical"
        android:checked="false"
        android:textSize="30dp"
        android:layout_weight="0.13" />

    <RadioButton

        android:textStyle="bold"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="@string/essence"
        android:id="@+id/essence"
        android:paddingLeft="10dp"
        android:paddingRight="50dp"
        android:layout_gravity="left|center_vertical"
        android:checked="true"
        android:textSize="30dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/precedent"
        android:background="@drawable/previous"
        android:padding="40dp"
        android:layout_alignTop="@+id/terminer"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="20dp"
        android:layout_gravity="left|bottom" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/terminer"
        android:background="@drawable/finish"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/adresse"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:padding="40dp"
        android:layout_marginRight="20dp"
        android:layout_gravity="right|bottom" />

</FrameLayout>
